<div  id="typewriter"></div>

<script type="text/javascript">
  var i = 0;
  var txt = 'Leave the Burden, Live the Adventure.';
  var speed = 50;
  function typeWriter() {
    if (i < txt.length) {
      document.getElementById("typewriter").innerHTML += txt.charAt(i);
      i++;
      setTimeout(typeWriter, speed);
    }
  }
typeWriter()

</script>




